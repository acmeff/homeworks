


<form action="<%= action_url %>" method="<%=method%>">
  <%= auth_token %>

  <label>Email
    <input type="text" name="user[email]" value="<%= user.email %>">
  </label>

  <label>Password
    <input type="password" name="user[password]" value="<%= user.password %>">
  </label>

  <input type="submit" value="Submit">
</form>
